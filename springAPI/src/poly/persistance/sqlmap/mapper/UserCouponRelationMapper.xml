<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- JAVA와 연결할 Mapper 파일 설정 -->
<mapper namespace="poly.persistance.mapper.IUserCouponRelationMapper">
	<select id="findRelation" resultType="UserCouponRelationDTO" parameterType="UserDTO">
			SELECT UCR_COUPON_SEQ
			FROM OnlineCoupon.USER_COUPON_RELATION
			WHERE UCR_USER_SEQ = #{user_seq}

	</select>


			
</mapper>